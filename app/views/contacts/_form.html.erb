<%= form_with model: contact, local: true do |f| %>
  <div class="form-group">
    <%= f.label :グループ名 %>
    <%= f.text_field :title, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :紹介文 %>
    <%= f.text_area :contact, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.submit '送信', class: "btn btn-info" %>
  </div>
<% end %>